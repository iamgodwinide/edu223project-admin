<div class="main-content">
    <div class="section__content section__content--p30">
        <div class="container-fluid bg-white p-3">
            <div class="row">
                <div class="col-12">
                    <h1 class="mb-5">Result List</h1>
                    <div class="table-responsive table--no-card m-b-30 p-3">
                        <table class="table table-borderless table-striped table-earning" id="students">
                            <thead>
                                <tr>
                                    <th class="text-left">Mat Number</th>
                                    <th class="text-left">Score</th>
                                    <th class="text-left">Grade</th>
                                </tr>
                            </thead>
                            <tbody>
                                <% results.map(r=> {%>
                                    <tr>
                                        <td class="text-left">
                                            <%= r.matno %>
                                        </td>
                                        <td class="text-left">
                                            <%= r.score %>
                                        </td>
                                        <td class="text-left">
                                            <%= r.grade %>
                                        </td>
                                    </tr>
                                    <%})%>
                            </tbody>
                        </table>
                    </div>
                    <hr>
                    <h3 class="my-4">Danger</h3>
                    <form action="<%= id%>/delete" method="post">
                        <button class="btn btn-danger">Delete All</button>
                    </form>
                </div>
            </div>
        </div>
    </div>
</div>