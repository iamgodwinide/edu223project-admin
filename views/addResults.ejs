<div class="main-content">
    <div class="section__content section__content--p30">
        <div class="container-fluid bg-white p-3">
            <h2 class="mb-2">Upload Results</h2>

            <div class="row">
                <div class="col-lg-12">
                    <div class="card">
                        <div class="card-body card-block">
                            <p class="mb-5 mt-2">
                                <i class="fas fa-info-circle"></i>
                                Reuploading will update/replace previous data.
                            </p>

                            <form method="post" enctype="multipart/form-data">
                                <%- include("./partials/messages") %>
                                    <div class="row">
                                        <div class="form-group col-md-4">
                                            <label for="course_code" class=" form-control-label">Course Code</label>
                                            <input type="text" name="course" id="course_code" placeholder="EDU223"
                                                class="form-control" required maxlength="6">
                                        </div>
                                        <div class="form-group col-md-4">
                                            <label for="credit" class=" form-control-label">Credit</label>
                                            <select class="form-control" required name="credit">
                                                <option value="">select</option>
                                                <option value="1">1</option>
                                                <option value="2">2</option>
                                                <option value="3">3</option>
                                                <option value="4">4</option>
                                                <option value="5">5</option>
                                                <option value="6">6</option>
                                                <option value="7">7</option>
                                            </select>
                                        </div>
                                        <div class="form-group col-md-4">
                                            <label for="dept" class=" form-control-label">Department</label>
                                            <select class="form-control" required name="department">
                                                <option value="">select</option>
                                                <option value="ADT">ADT</option>
                                                <option value="CIT">CIT</option>
                                                <option value="DEF">DEF</option>
                                                <option value="DEM">DEM</option>
                                                <option value="EECP">EECP</option>
                                                <option value="HSE">HSE</option>
                                                <option value="HKS">HKS</option>
                                                <option value="VTE">VTE</option>
                                            </select>
                                        </div>
                                        <div class="form-group col-md-4">
                                            <label for="matno" class=" form-control-label">Session</label>
                                            <select class="form-control" required name="session">
                                                <option value="">select</option>
                                                <option value="2020/2021">2020/2021</option>
                                                <option value="2019/2020">2019/2020</option>
                                                <option value="2018/2019">2018/2019</option>
                                            </select>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="form-group col-md-6">
                                            <label for="matno" class=" form-control-label">Level</label>
                                            <select class="form-control" required name="level">
                                                <option value="">select</option>
                                                <option value="100">100</option>
                                                <option value="200">200</option>
                                                <option value="300">300</option>
                                                <option value="400">400</option>
                                            </select>
                                        </div>
                                        <div class="form-group col-md-6">
                                            <label for="matno" class=" form-control-label">Semester</label>
                                            <select class="form-control" required name="semester">
                                                <option value="">select</option>
                                                <option value="first">First Semester</option>
                                                <option value="second">Second Semester</option>
                                            </select>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label for="csv">Results(CSV)</label>
                                        <br />
                                        <small>
                                            The csv file must include the following headings: <b>Score</b>,
                                            <b>Matno</b> and must be saved as a CSV(comma delimited) file.
                                        </small>
                                        <input type="file" class="form-control" required name="csv" accept="text/csv">
                                    </div>
                                    <div class="form-group">
                                        <button class="btn btn-success">Upload</button>
                                    </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>